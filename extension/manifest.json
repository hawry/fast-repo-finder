{
  "manifest_version": 2,
  "name": "Fast Repository Finder",
  "description": "WebExtension for Chrome & Firefox to quickly allow you to query popular software package sites for libraries & dependencies without going through google or their respective user interfaces",
  "version": "0.1.0",
  "permissions": ["storage"],
  "icons": {
    "16": "/images/16x16.png",
    "32": "/images/32x32.png",
    "48": "/images/48x48.png",
    "64": "/images/64x64.png",
    "128": "/images/128x128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "/images/16x16.png",
      "24": "/images/24x24.png",
      "32": "/images/32x32.png"
    },
    "default_title": "Fast Repository Finder",
    "default_popup": "popup/index.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{c8fdbcaa-87c2-11ea-b978-2b4189abc913}"
    }
  }
}
