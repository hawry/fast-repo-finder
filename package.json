{
  "name": "fast-repo-finder",
  "version": "0.1.1",
  "description": "WebExtension for Chrome & Firefox to quickly allow you to query popular software package sites for libraries & dependencies without going through google or their respective user interfaces.",
  "main": "index.js",
  "scripts": {
    "test": "karma start",
    "test:debug": "karma start --no-single-run --auto-watch",
    "test:headless": "karma start --browsers FirefoxHeadless,ChromeHeadless",
    "web-ext": "web-ext run -s ./extension --keep-profile-changes",
    "build": "web-ext build -s ./extension --ignore-files=raw/ web-ext-artifactes/"
  },
  "repository": "git@github.com:hawry/fast-repo-finder",
  "author": "hawry <tomas@hawry.net>",
  "license": "All Rights Reserved",
  "private": true,
  "devDependencies": {
    "chai": "^4.2.0",
    "expect.js": "^0.3.1",
    "karma": "^5.0.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-firefox-launcher": "^1.3.0",
    "karma-mocha": "^2.0.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon-chrome": "^0.2.0",
    "mocha": "^7.1.1",
    "sinon-chrome": "^3.0.1",
    "web-ext": "^4.1.0"
  }
}
